[32m+[m[32mconst server = http.createServer((req, res) => {[m
[32m+[m[32m    if (req.url == "/") {[m
[32m+[m[32m        res.write("<h1>Home Page</h1>");[m
[32m+[m[32m        res.end();[m
[32m+[m[32m    } else if (req.url == "/about") {[m
[32m+[m[32m        res.write("<h1>About Page</h1>");[m
[32m+[m[32m        res.end();[m
[32m+[m[32m    } else if (req.url == "/contact") {[m
[32m+[m[32m        res.write("<h1>Contact Page</h1>");[m
[32m+[m[32m        res.end();[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // set status code to 404[m
[32m+[m[32m        res.statusCode = 404;[m
[32m+[m[32m        res.write("<h1>404 | Page not found</h1>");[m
[32m+[m[32m        res.end();[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
